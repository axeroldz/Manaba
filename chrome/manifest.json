{
    "name": "Manaba Test",
    "version": "0.1.0",
    "permissions": ["storage","tabs"],
    "host_permissions": ["https://manaba.ryukoku.ac.jp/ct/*"],
    "content_scripts": [
      {
        "matches": [
          "https://manaba.ryukoku.ac.jp/ct/home_course*",
          "https://manaba.ryukoku.ac.jp/ct/home*",
          "https://manaba.ryukoku.ac.jp/ct/home?chglistformat=*"
        ],
        "run_at": "document_idle",
        "js": ["/js/Sort.js","/js/content.js"]
      },
      {
        "matches": ["https://manaba.ryukoku.ac.jp/ct/course*_report_*"],
        "js": ["/js/drop.js"]
      },
      {
        "matches":["https://manaba.ryukoku.ac.jp/ct/home",
        "https://manaba.ryukoku.ac.jp/ct/home?*"],
        "js":["/js/accUIhome.js"]
      },
      {
        "matches":["https://manaba.ryukoku.ac.jp/ct/home_course*"],
        "js":["/js/accUIcourse.js"]
      }
    ],
    "background": {
      "service_worker": "/js/background.js"
    },
    "action": {
      "default_icon": {
        "16": "logo/icon.png",
        "24": "logo/icon.png",
        "32": "logo/icon.png"
      },
      "default_title": "My Manaba",
      "default_popup": "/popup/popup.html"
    },
    "manifest_version": 3
  }
  

